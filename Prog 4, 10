Program 4:(Interface)

using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
namespace program_4_charp
{
interface GFG1
{
void details();
}
interface GFG2
{
int marks();
}
class inter : GFG1, GFG2
{
public void details()
{
int r;
string n;
Console.WriteLine("Enter your Roll Number:");
r = int.Parse(Console.ReadLine());
Console.WriteLine("Enter your Name :");
n = Console.ReadLine();
}
public int marks()
{
int m1, m2, m3, t;
Console.WriteLine("Enter your first Subject Mark:");
m1 = int.Parse(Console.ReadLine());
Console.WriteLine("Enter your second Subject Mark:");
m2 = int.Parse(Console.ReadLine());
Console.WriteLine("Enter your third Subject Mark:");
m3 = int.Parse(Console.ReadLine());
t = m1 + m2 + m3;
return t;
}
}
class Program
{
static void Main(string[] args)
{
inter obj = new inter();
obj.details();
int tot = obj.marks();
float p = tot / 3.0f;
Console.WriteLine("Total =" + tot);
Console.ReadLine();
Console.WriteLine("Percentage =" + p);
Console.ReadLine();
}
}}

